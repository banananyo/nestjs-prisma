{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "gen-type": "ts-node src/generate-typings.ts",
    "format": "prettier --check --write 'src/**/*.ts'",
    "lint": "eslint --fix --ext .ts 'src/**/*.ts",
    "fix": "tsc --noEmit && yarn lint",
    "prepare": "husky install",
    "commit": "git-cz",
    "prebuild": "rimraf dist/*",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dollarsign/logger": "^1.4.0",
    "@nestjs/common": "^8.0.9",
    "@nestjs/core": "^8.0.9",
    "@nestjs/graphql": "^9.0.5",
    "@nestjs/platform-express": "^8.0.9",
    "@prisma/client": "^2.26.0",
    "apollo-server": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.6.0",
    "graphql-subscriptions": "^1.2.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "prisma": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@apollo/gateway": "^0.42.0",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.9",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/luxon": "^1.27.0",
    "@types/node": "^16.10.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-morph": "^12.0.0",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
